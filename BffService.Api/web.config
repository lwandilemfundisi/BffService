<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<proxy>
		<reverseProxy enabled="true"/>
	</proxy>
	<system.webServer>
		<handlers>
			<remove name="WebDAV"/>
			<remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified"/>
		</handlers>
		<aspNetCore
			processPath="dotnet" arguments="BffService.Api.dll" stdoutLogEnabled="false" hostingModel="outofprocess"/>
	</system.webServer>
</configuration>